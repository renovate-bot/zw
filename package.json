{
  "name": "zw",
  "version": "1.0.0",
  "private": true,
  "description": "ZI & Zsh Wiki",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z-shell/zw.git"
  },
  "author": "Salvydas Lukosius",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/z-shell/zw/issues"
  },
  "homepage": "https://github.com/z-shell",
  "scripts": {
    "docusaurus": "docusaurus",
    "preinstall": "npx only-allow pnpm",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "build:cf": "npx pnpm i --store=node_modules/.pnpm-store --no-frozen-lockfile && npx pnpm run build",
    "build:cu": "curl -f https://get.pnpm.io/v6.16.js | node - add --global pnpm && pnpm i --store=node_modules/.pnpm-store && pnpm build",
    "build:en": "docusaurus build --locale en",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-docs-version": "docusaurus docs:version",
    "write-community-version": "docusaurus docs:version:community",
    "format:text:check": "markdownlint-cli2 '**/*.{md,mkd,mdwn,mdown,markdown}' '#node_modules'",
    "format:text:fix": "markdownlint-cli2-fix '**/*.{md,mkd,mdwn,mdown,markdown}' '#node_modules'",
    "format:check": "prettier --check '**/*.{js,css,json,md,ts,tsx,yml,yaml}'",
    "format:write": "prettier --write '**/*.{js,css,json,md,ts,tsx,yml,yaml}'",
    "format:diff": "prettier --list-different . ",
    "crowdin": "crowdin",
    "crowdin:sync": "docusaurus write-translations && crowdin upload && crowdin download",
    "crowdin:contributors": "crowdin-contributors -f 'CONTRIBUTORS.md' -o 'digitalclouds' -p '2' -t"
  },
  "dependencies": {
    "@awesome-crowdin/crowdin-contributors": "^0.0.4",
    "@docusaurus/core": "2.0.0-beta.16",
    "@docusaurus/mdx-loader": "^2.0.0-beta.16",
    "@docusaurus/module-type-aliases": "^2.0.0-beta.16",
    "@docusaurus/plugin-client-redirects": "^2.0.0-beta.16",
    "@docusaurus/plugin-content-blog": "^2.0.0-beta.16",
    "@docusaurus/plugin-content-docs": "^2.0.0-beta.16",
    "@docusaurus/plugin-content-pages": "^2.0.0-beta.16",
    "@docusaurus/plugin-debug": "^2.0.0-beta.16",
    "@docusaurus/plugin-ideal-image": "^2.0.0-beta.16",
    "@docusaurus/plugin-pwa": "^2.0.0-beta.16",
    "@docusaurus/plugin-sitemap": "^2.0.0-beta.16",
    "@docusaurus/preset-classic": "0.0.0-4644",
    "@docusaurus/theme-classic": "^2.0.0-beta.16",
    "@docusaurus/theme-search-algolia": "^2.0.0-beta.16",
    "@docusaurus/theme-translations": "^2.0.0-beta.16",
    "@docusaurus/types": "^2.0.0-beta.16",
    "@docusaurus/utils": "^2.0.0-beta.16",
    "@docusaurus/utils-common": "^2.0.0-beta.16",
    "@docusaurus/utils-validation": "^2.0.0-beta.16",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.1.1",
    "prism-react-renderer": "^1.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@algolia/client-search": "^4.12.1",
    "@babel/core": "^7.17.5",
    "@babel/plugin-syntax-typescript": "^7.16.7",
    "@crowdin/cli": "3.7.8",
    "@netlify/plugin-lighthouse": "^2.1.3",
    "@tsconfig/docusaurus": "1.0.4",
    "@types/react": "^17.0.39",
    "markdownlint-cli2": "^0.4.0",
    "prettier": "2.5.1",
    "prop-types": "^15.8.1",
    "stylelint": "^14.5.3",
    "typescript": "4.5.5"
  },
  "engines": {
    "node": ">=16",
    "yarn": ">=3",
    "pnpm": ">=6"
  },
  "pnpm": {
    "overrides": {
      "nth-check@<2.0.1": ">=2.0.1",
      "trim@<0.0.3": ">=0.0.3"
    }
  },
  "peerDependencies": {
    "@algolia/client-search": "^4.12.1",
    "@types/react": "^17.0.39"
  }
}
