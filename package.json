{
  "name": "zw",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "build:en": "docusaurus build --locale en",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-docs-version": "docusaurus docs:version",
    "write-community-version": "docusaurus docs:version:community",
    "md:check": "markdownlint-cli2 '**/*.md' '#node_modules'",
    "md:fix": "markdownlint-cli2-fix '**/*.md' '#node_modules'",
    "prettier": "prettier --config .prettierrc --ignore-path .gitignore --ignore-path '.prettierignore' '**/*.{js,jsx,ts,tsx,mdx,yml,yaml}'",
    "crowdin:sync": "docusaurus write-translations && crowdin upload && crowdin download",
    "build:cf:pre": "corepack enable && corepack prepare pnpm@$PNPM_VERSION --activate && pnpm build:cf",
    "build:cf": "pnpm install --strict-peer-dependencies --no-prefer-frozen-lockfile && pnpm build",
    "w:dev": "wrangler pages dev build -l --live-reload",
    "w:pub": "wrangler publish",
    "w:build:pub": "pnpm -s install && pnpm -s build && pnpm -s wrangler publish",
    "kv:list": "wrangler kv:namespace list | jq '.'",
    "preinstall": "npm config set store-dir '~/.pnpm-store'",
    "prepare": "husky install",
    "clean": "rm -rf node_modules",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "0.0.0-4727",
    "@docusaurus/plugin-ideal-image": "0.0.0-4727",
    "@docusaurus/plugin-pwa": "0.0.0-4727",
    "@docusaurus/preset-classic": "0.0.0-4727",
    "@docusaurus/theme-search-algolia": "0.0.0-4727",
    "@mdx-js/react": "1.6.22",
    "clsx": "1.1.1",
    "prism-react-renderer": "1.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@algolia/client-search": "4.13.0",
    "@babel/core": "7.17.8",
    "@babel/plugin-syntax-typescript": "7.16.7",
    "@cloudflare/kv-asset-handler": "0.2.0",
    "@cloudflare/workers-types": "3.4.0",
    "@crowdin/cli": "3.7.8",
    "@docusaurus/module-type-aliases": "0.0.0-4727",
    "@tsconfig/docusaurus": "1.0.5",
    "@types/react": "^17.0.41",
    "husky": "^7.0.4",
    "markdownlint-cli2": "^0.4.0",
    "prettier": "2.6.0",
    "pretty-quick": "^3.1.3",
    "prop-types": "15.8.1",
    "stylelint": "14.6.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "typescript": "4.6.2",
    "wrangler": "^0.0.22"
  },
  "engines": {
    "node": ">=14",
    "pnpm": ">=6"
  },
  "pnpm": {
    "overrides": {
      "nth-check@<2.0.1": ">=2.0.1",
      "trim@<0.0.3": ">=0.0.3",
      "minimist@<=1.2.5": "<0.0.0"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged; pnpm -s md:fix"
    }
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
