{
  "name": "zw",
  "version": "1.0.0",
  "private": true,
  "description": "Zsh Wiki",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z-shell/zw.git"
  },
  "author": "Salvydas Lukosius",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/z-shell/zw/issues"
  },
  "homepage": "https://github.com/z-shell",
  "scripts": {
    "pre-commit": "pre-commit",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "build:en": "docusaurus build --locale en",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-docs-version": "docusaurus docs:version",
    "write-community-version": "docusaurus docs:version:community",
    "md:check": "markdownlint-cli2 '**/*.md' '#node_modules'",
    "md:fix": "markdownlint-cli2-fix '**/*.md' '#node_modules'",
    "prettier": "prettier --config .prettierrc --ignore-path .gitignore --ignore-path '.prettierignore' '**/*.{js,jsx,ts,tsx,mdx,yml,yaml}'",
    "eslint": "eslint --ignore-path .gitignore --ignore-path .prettierignore --cache --report-unused-disable-directives '**/*.{js,jsx,ts,tsx,mjs}'",
    "fix": "pnpm eslint --fix && pnpm prettier --write && pnpm markdownlint --fix",
    "crowdin:sync": "docusaurus write-translations && crowdin upload && crowdin download",
    "build:cf:pre": "corepack enable && corepack prepare pnpm@$PNPM_VERSION --activate && pnpm build:cf",
    "build:cf": "pnpm install --strict-peer-dependencies --prefer-frozen-lockfile && pnpm build && pnpm w:pub",
    "w:dev": "wrangler pages dev build -l --live-reload",
    "w:pub": "wrangler publish",
    "w:build:pub": "pnpm -s install && pnpm -s build && pnpm -s wrangler publish",
    "kv:list": "wrangler kv:namespace list | jq '.'",
    "preinstall": "npm config set store-dir '~/.pnpm-store'",
    "postinstall": "pnpm -s audit --fix",
    "prepare": "husky install",
    "clean": "rm -rf node_modules"
  },
  "dependencies": {
    "@docusaurus/core": "0.0.0-4675",
    "@docusaurus/module-type-aliases": "0.0.0-4675",
    "@docusaurus/plugin-ideal-image": "0.0.0-4675",
    "@docusaurus/plugin-pwa": "0.0.0-4675",
    "@docusaurus/preset-classic": "0.0.0-4675",
    "@docusaurus/theme-search-algolia": "0.0.0-4675",
    "@mdx-js/react": "1.6.22",
    "clsx": "1.1.1",
    "prism-react-renderer": "1.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.17.8",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-syntax-typescript": "7.16.7",
    "@cloudflare/workers-types": "3.4.0",
    "@tsconfig/docusaurus": "1.0.5",
    "@types/react": "17.0.41",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "2.6.0",
    "prop-types": "15.8.1",
    "stylelint": "14.6.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "@algolia/client-search": "4.13.0",
    "@cloudflare/kv-asset-handler": "0.2.0",
    "@crowdin/cli": "3.7.8",
    "husky": "^7.0.4",
    "markdownlint-cli2": "^0.4.0",
    "pre-commit": "^1.2.2",
    "pretty-quick": "^3.1.3",
    "typescript": "4.6.2",
    "wrangler": "^0.0.19"
  },
  "engines": {
    "node": ">=14",
    "pnpm": ">=6"
  },
  "pnpm": {
    "overrides": {
      "nth-check@<2.0.1": ">=2.0.1",
      "trim@<0.0.3": ">=0.0.3"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged; pnpm -s md:fix"
    }
  },
  "peerDependencies": {
    "@algolia/client-search": "^4.12.1",
    "@types/react": "^17.0.39"
  }
}
