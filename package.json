{
  "name": "zw",
  "version": "1.0.0",
  "private": true,
  "description": "Zsh Wiki",
  "main": "src/workers/service.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z-shell/zw.git"
  },
  "author": "Salvydas Lukosius",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/z-shell/zw/issues"
  },
  "homepage": "https://github.com/z-shell",
  "scripts": {
    "husky": "husky",
    "crowdin": "crowdin",
    "wrangler": "wrangler",
    "pre-commit": "pre-commit",
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus build",
    "build:en": "docusaurus build --locale en",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-docs-version": "docusaurus docs:version",
    "write-community-version": "docusaurus docs:version:community",
    "lint:check": "markdownlint-cli2 \"**/*.{md,mkd,mdwn,mdown,markdown}\" \"#node_modules\"",
    "lint:fix": "markdownlint-cli2-fix \"**/*.{md,mkd,mdwn,mdown,markdown}\" \"#node_modules\"",
    "format": "prettier --config .prettierrc --write \"**/*.{js,jsx,ts,tsx,md,mdx,yml,yaml}\"",
    "crowdin:sync": "docusaurus write-translations && crowdin upload && crowdin download",
    "preinstall": "npx only-allow pnpm",
    "build:cf": "npx pnpm pre:pnpm && npx pnpm build && npx pnpm w:pub",
    "get:pnpm": "curl -f 'https://get.pnpm.io/v6.16.js' | node - add '--global pnpm'",
    "pre:pnpm": "npx pnpm i '--store=node_modules/.pnpm-store'",
    "w:dev": "npx pnpm wrangler pages dev build -l --live-reload",
    "w:pub": "npx pnpm wrangler publish",
    "w:build:pub": "pnpm i && pnpm build && pnpm wrangler publish",
    "kv:list": "wrangler kv:namespace list | jq '.'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@docusaurus/core": "0.0.0-4675",
    "@docusaurus/module-type-aliases": "0.0.0-4675",
    "@docusaurus/plugin-ideal-image": "0.0.0-4675",
    "@docusaurus/plugin-pwa": "0.0.0-4675",
    "@docusaurus/preset-classic": "0.0.0-4675",
    "@docusaurus/theme-search-algolia": "0.0.0-4675",
    "@mdx-js/react": "1.6.22",
    "clsx": "1.1.1",
    "prism-react-renderer": "1.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@algolia/client-search": "4.12.2",
    "@babel/core": "7.17.5",
    "@babel/plugin-syntax-typescript": "7.16.7",
    "@cloudflare/kv-asset-handler": "0.2.0",
    "@cloudflare/workers-types": "3.4.0",
    "@crowdin/cli": "3.7.8",
    "@tsconfig/docusaurus": "1.0.4",
    "@types/react": "17.0.40",
    "git-format-staged": "^2.1.3",
    "husky": "^7.0.0",
    "markdownlint-cli2": "0.4.0",
    "miniflare": "^2.3.0",
    "pre-commit": "^1.2.2",
    "prettier": "2.5.1",
    "prop-types": "15.8.1",
    "stylelint": "14.5.3",
    "typescript": "4.6.2",
    "wrangler": "beta"
  },
  "engines": {
    "node": ">=14",
    "pnpm": ">=6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm format"
    }
  },
  "pnpm": {
    "overrides": {
      "nth-check@<2.0.1": ">=2.0.1",
      "trim@<0.0.3": ">=0.0.3"
    }
  },
  "peerDependencies": {
    "@algolia/client-search": "^4.12.1",
    "@types/react": "^17.0.39"
  }
}
